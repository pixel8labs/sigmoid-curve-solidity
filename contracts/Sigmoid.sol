pragma solidity ^0.8.0;

import "./BytesLib.sol";

library Sigmoid {
    bytes constant sigmoidValues = hex"";

    function getSigmoidValue(uint index) public pure returns (uint32) {
        require(index < 168, "Index out of bounds");
        return BytesLib.toUint32(sigmoidValues, index* 4);
    }
}
